@model IEnumerable<GrowIndigo.DAL.DataEntities.shop_cmp_image>

@using GrowIndigo.Utilities.Resources;
@using GrowIndigo.DAL.DataModels.CommonModels;
@using GrowIndigo.Utilities;

@{
    var Projectname = ConfigReader.ProjectName;
    var CategoryName = "N/A";
    var State = "0";
    var search = "N/A";
    LoginViewModel loggedInUser = Session["LoggedInUser"] != null ? (LoginViewModel)Session["LoggedInUser"] : null;
}


<div class="col-lg-3">
    <div class="all-category">
        <h3 class="cat-heading"><i class="fa fa-bars" aria-hidden="true"></i>@Resource.GrowOnline_Common_CompanyProduct</h3>
        <ul class="main-category">
            @foreach (var companyProduct in Model.Select(s => s.cmpName).Distinct())
            {
                    <li><a href="@Projectname/ProductMaster/GetProductFilter?CategoryName=@CategoryName&State=@State&search=@search&CompanyName=@companyProduct">@companyProduct</a></li>
                }


        </ul>
    </div>
</div>

